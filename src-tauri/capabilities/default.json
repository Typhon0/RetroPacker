{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/chdman",
          "cmd": "program",
          "sidecar": true
        }
      ]
    },
    "shell:default",
    "store:default"
  ]
}